{
  "name": "the-content-farm-terminator",
  "version": "3.2",
  "description": "A chrome plugin for blocking content farm results on search pages.",
  "author": {
    "name": "hyperbola",
    "email": "me@hyperbola.me",
    "url": "https://github.com/wdzeng"
  },
  "homepage": "https://chrome.google.com/webstore/detail/the-content-farm-terminat/chhekpgdckchblnfdelceaigmlfbakgn",
  "repository": {
    "type": "git",
    "url": "https://github.com/wdzeng/The-Content-Farm-Terminator.git"
  },
  "scripts": {
    "build": "yarn dev && yarn zip",
    "dev": "yarn transpile",
    "transpile": "tsc --rootDir ./src/scripts",
    "zip": "zip -r The-Content-Farm-Terminator.zip ./src -x *.ts",
    "clean": "rm -f ./src/scripts/*.js ./The-Content-Farm-Terminator.zip"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "@types/chrome": "0.0.177",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1"
  },
  "bugs": {
    "url": "https://github.com/wdzeng/The-Content-Farm-Terminator/issues"
  },
  "license": "MIT"
}